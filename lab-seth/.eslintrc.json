{
  "env": {
    "browser": true,
    "node": true,
    "commonjs": true,
    "jest": true,
    "es6": true
  },
  "globals": {
    "err": true,
    "req": true,
    "res": true,
    "next": true
  },
    "extends": ["eslint:recommended", "plugin:react/recommended"],
"plugins": ["react"],
"parserOptions": {
  "sourceType": "module",
  "ecmaFeatures": {
    "experimentalObjectRestSpread": true,
    "module": true,
    "jsx": true
  }
},
  "rules": {
    "no-console": "off",
    "react/prop-types": [0],
    "indent": [ "error", 2 ],
    "quotes": ["error", "single", { "allowTemplateLiterals": true }],
    "space-infix-ops": ["error", {"int32Hint": false}],
    "comma-dangle": ["error", "always-multiline"],
    "semi": [ "error", "always" ]
  }
}